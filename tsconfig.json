{
  "compilerOptions": {
    "target": "ES2020",


    "module": "ESNext",

    "moduleResolution": "Node",


    "jsx": "react",


    "allowJs": true,


    "checkJs": false,


    "esModuleInterop": true,


    "forceConsistentCasingInFileNames": false,


    "strict": false,


    "skipLibCheck": true,
    

    "types": ["node", "jest"]
  },


  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./types/env.d.ts" 
  ],
}